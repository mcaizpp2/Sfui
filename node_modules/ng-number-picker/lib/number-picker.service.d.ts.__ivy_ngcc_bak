export declare class NumberPickerService {
    /**
     * Min picker value
     */
    min: number;
    /**
     * Max picker value
     */
    max: number;
    /**
     * Pick step value
     */
    step: number;
    /**
     * Delay for start picking values
     */
    pickStartAfter: number;
    /**
     * Delay betweens each pick
     */
    pickTimer: number;
    /**
     * value precision
     */
    precision: number;
    /**
     * Inital picker value
     */
    value: any;
}
